		<jsp-file>/vues/afficheGaulois.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>afficheGaulois</servlet-name>
		<url-pattern>/afficheGaulois</url-pattern>
	</servlet-mapping>
	
	
</web-app>
