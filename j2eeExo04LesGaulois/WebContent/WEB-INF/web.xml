	</servlet-mapping>
	
	<servlet>
		<servlet-name>specialites</servlet-name>
		<jsp-file>/vues/lesSpecialites.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>specialites</servlet-name>
		<url-pattern>/lesSpecialites</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>potions</servlet-name>
		<jsp-file>/vues/lesPotions.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>potions</servlet-name>
		<url-pattern>/lesPotions</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>lieux</servlet-name>
		<jsp-file>/vues/lesLieux.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>lieux</servlet-name>
		<url-pattern>/lesLieux</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>casques</servlet-name>
		<jsp-file>/vues/lesCasques.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>casques</servlet-name>
		<url-pattern>/lesCasques</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>req_params</servlet-name>
		<jsp-file>/vues/req_params.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>req_params</servlet-name>
		<url-pattern>/req_params</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RechGaulois</servlet-name>
	</servlet>
	<servlet-mapping>
		<servlet-name>RechGaulois</servlet-name>
		<url-pattern>/RechGaulois</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Recherche</servlet-name>
	</servlet>
	<servlet-mapping>
		<servlet-name>Recherche</servlet-name>
		<url-pattern>/Recherche</url-pattern>
	</servlet-mapping>
	
	<servlet>
